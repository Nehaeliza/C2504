StringInterpolation

using System;
class Program
{
    
    static void Main(string[] args) //user: p
    {
        string name = "Neha";
        int age = 22;
        Console.WriteLine("Name:{0}, age:{1}\n",name, age);
        Console.WriteLine($"Name:{name}, age:{age}");          // Name:Neha, age:22
        Console.ReadKey();
    }        
}
